    "next": "14.2.4",
    "next-ws": "^1.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ws": "^8.16.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.11",
    "@types/cookie": "^0.6.0",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^20.14.9",
    "@types/react": "18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/ws": "^8.5.10",
    "dotenv": "^16.4.5",
