  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "node .next/standalone/server.js",
    "deploy": "rm -rf .next && next build && cp -r .next/static .next/standalone/.next && cp -r drizzle .next/standalone/drizzle && yarn start",
    "lint": "next lint",
    "db:init": "npx drizzle-kit generate --name init_db",
    "db:migrate": "npx drizzle-kit migrate",
    "db:studio": "npx drizzle-kit studio",
    "postinstall": "npx next-ws-cli@latest patch"
  },
  "dependencies": {
    "better-sqlite3": "^11.1.2",
    "cookie": "^0.6.0",
    "drizzle-orm": "^0.31.2",
    "install": "^0.13.0",
